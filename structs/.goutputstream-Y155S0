#include <bits/stdc++.h>

using namespace std;

template <typename T>
class Dianamic_array
{
private:
    int *a;
    int size;
    void change_size();
public:
    Dianamic_array::Dianamic_array(int len)
    {
        a = new int[len];
        size = len;
    }
    ~Dianamic_array::Dianamic_array()
    {
        delete *a;
    }
    int size();
    void resize();
    T get_item(int ind);
};
